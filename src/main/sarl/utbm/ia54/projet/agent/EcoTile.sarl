package utbm.ia54.projet.^agent

import io.sarl.core.Initialize
import io.sarl.core.Lifecycle
import utbm.ia54.projet.message.UUIDMessage

agent EcoTile extends EcoAgent {

	uses Lifecycle
	
	on Initialize {
		// Initialize parameters
		num = occurrence.parameters.get(0) as Integer
	}
	
	on UUIDMessage {
		idLinked = occurrence.id
		
		// Debug
		println("Tile"+num+" | Id:"+ID+" | Linked:"+idLinked)
		
		killMe
	}
}